{
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "jsx": true,
      "spread": true
    },
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "es6": true,
    "jest": true,
    "node": true
  },
  "extends": [
    "eslint:recommended", "plugin:react/recommended"
  ],
  "plugins": ["react"],
  "rules": {
    "semi": [2, "always"],
    "quotes": [2, "single"],
    "jsx-quotes": [2, "prefer-double"]
  },
  "overrides": [{
    "files": ["Home.jsx"],
    "rules": {
      "react/prop-types": 0
    }
  }]
}